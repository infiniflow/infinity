CREATE TABLE test_knn_l2(c1 INT, c2 EMBEDDING(FLOAT, 4));

# copy to create one block
# the csv has 4 rows, the l2 distance to target([0.3, 0.3, 0.2, 0.2]) is:
# 1. 0.2^2 + 0.1^2 + 0.1^2 + 0.4^2 = 0.22
# 2. 0.1^2 + 0.2^2 + 0.1^2 + 0.2^2 = 0.1
# 3. 0 + 0.1^2 + 0.1^2 + 0.2^2 = 0.06
# 4. 0.1^2 + 0 + 0 + 0.1^2 = 0.02
statement ok
COPY test_knn_l2 FROM '/tmp/infinity/test_data/embedding_float_dim4.csv' WITH (DELIMITER ',');

# mertic l2 will order ascendingly. The query will return row 4, 3, 2
query I
SELECT c1 FROM test_knn_l2 SEARCH KNN(c2, [0.3, 0.3, 0.2, 0.2], 'float', 'l2', 3);
----
8
6
4

query I
SELECT c2 FROM test_knn_l2 SEARCH KNN(c2, [0.3, 0.3, 0.2, 0.2], 'float', 'l2', 3);
----
0.4,0.3,0.2,0.1
0.3,0.2,0.1,0.4
0.2,0.1,0.3,0.4

query II
SELECT c1, ROW_ID(), DISTANCE() FROM test_knn_l2 SEARCH KNN(c2, [0.3, 0.3, 0.2, 0.2], 'float', 'l2', 3);
----
8 3 0.020000
6 2 0.060000
4 1 0.100000


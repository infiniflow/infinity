# EPEL 8 - China Mirrors Configuration
# This file provides faster EPEL mirror sources for users in China

[epel]
name=Extra Packages for Enterprise Linux $releasever - $basearch
baseurl=https://mirrors.aliyun.com/epel/$releasever/Everything/$basearch
        https://mirrors.tuna.tsinghua.edu.cn/epel/$releasever/Everything/$basearch
        https://mirrors.ustc.edu.cn/epel/$releasever/Everything/$basearch
        https://mirror.sjtu.edu.cn/epel/$releasever/Everything/$basearch
enabled=1
gpgcheck=1
countme=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-8

[epel-debuginfo]
name=Extra Packages for Enterprise Linux $releasever - $basearch - Debug

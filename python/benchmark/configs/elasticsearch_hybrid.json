{
    "name": "elasticsearch_hybrid",
    "app": "elasticsearch",
    "app_path": "python/benchmark/servers/elasticsearch/",
    "connection_url": "http://localhost:9200",
    "data_path": "python/benchmark/datasets/dbpedia/dbpedia.json",
    "data_link": "to_be_set",
    "query_type": "json",
    "query_path": "python/benchmark/datasets/dbpedia/hybrid_queries.json",
    "query_link": "to_be_set",
    "insert_batch_size": 1024,
    "vector_name": "openai",
    "topK": 100,
    "index": {
      "settings": {
         "index.store.preload": [ "vec", "vex", "vem"],
         "index.number_of_shards": 1,
         "index.number_of_replicas": 0
       },
      "mappings": {
        "properties": {
         "title":{
            "type": "text"
         },
         "text":{
            "type": "text"
         },
         "openai": {
            "type": "dense_vector",
            "element_type": "float",
            "dims" : 1536,
            "index" : true,
            "similarity": "dot_product",
            "index_options": {
              "type": "hnsw"
            }
          }
        }
      }
    }
}